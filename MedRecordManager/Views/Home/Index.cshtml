@{
    ViewData["Title"] = "Home Page";
}

@section scripts{
    <script type="text/javascript">
        var processGrid;
        $(document).ready(function () {
          
            processGrid = $('#processTable').grid({
                primaryKey: 'processId',
                title: 'Weekly Auto Process Status',
                dataSource: '/Home/GetJobProcess',
                uiLibrary :'bootstrap',
                columns: [
                
                    { filed: 'processId', hidden: true },
                    { field: 'sourceFileName', minWidth: 600, title: 'File Name' },
                    { field: 'processResult', minWidth: 400, title: 'Result' },
                    { field: 'processedDate', width: 200, type: Date, title: 'Processed Date' },
                    { field: 'successFlag', width: 100, title: 'Succceed' },
                 
                ],
                pager: { limit: 10, sizes: [10] },
            });
          
        });

    </script>

}





<div class="row top-buffer">
    <div class="col-sm-10">
        <div class="table-responsive">
            <table id="processTable" class="table table-striped"></table>
        </div>
    </div>

</div>


